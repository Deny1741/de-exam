@import "../../styles/mixins.pcss";

.map {
    min-height: 300px;
    height: 100%;
    background: var(--c-gray);

    .hint {
        font-size: 14px;
        padding: 20px;
        line-height: 1.35;
        color: var(--c-gray);

        &__title {
            font-weight: bold;
            color: var(--c-dark);
        }
    }
}

